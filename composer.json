{
  "name": "yuncms/yii2-tag",
  "description": "The tag extension for the yii framework",
  "keywords": [
    "yuncms",
    "taggable",
    "tags",
    "tag"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "require": {
    "yiisoft/yii2": "~2.0.6",
    "yuncms/yii2-system": "~2.0",
    "creocoder/yii2-taggable": "~2.0",
    "xutl/yii2-bootstrap-tagsinput-asset": "~1.0.0"
  },
  "autoload": {
    "psr-4": {
      "yuncms\\tag\\": ""
    }
  },
  "extra": {
    "yuncms": {
      "name": "tag",
      "i18n": {
        "class": "yii\\i18n\\PhpMessageSource",
        "basePath": "@yuncms/tag/message"
      },
      "migrationNamespace": "@yuncms/tag/migrations",
      "backend": {
        "class": "yuncms\\tag\\backend\\Module"
      },
      "frontend": {
      }
    },
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  ]
}
